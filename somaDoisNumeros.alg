algoritmo "Soma de dois numeros"

// Autor : Franco Flores
// Data : 02/01/2021
var
   N1, N2, S : inteiro
inicio

   Escreva("Escreva o primeiro numero: ")
   Leia (N1)
   Escreva("Escreva o segundo numero: ")
   Leia (N2)
   
   S <- N1 + N2
   
   Escreva("A Soma entre ",N1," e ",N2," e igual a ", S)
fimalgoritmo
